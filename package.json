    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "browserslist": "^4.22.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression-webpack-plugin": "^10.0.0",
