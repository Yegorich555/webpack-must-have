    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "css-unicode-loader": "^1.0.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
